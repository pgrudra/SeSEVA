<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_to_sign_out"
    app:startDestination="@id/passageFragment">

    <fragment
        android:id="@+id/permissionFragment"
        android:name="com.example.us0.foregroundnnotifications.PermissionFragment"
        android:label="PermissionFragment" >
        <action
            android:id="@+id/action_permissionFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/navigation_to_sign_out"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_permissionFragment_to_DOOAFragment"
            app:destination="@id/DOOAFragment"
            app:popUpTo="@id/permissionFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/usageOverViewFragment"
        android:name="com.example.us0.home.usagestats.UsageOverviewFragment"
        android:label="UsageOverViewFragment" >
        <action
            android:id="@+id/action_usageOverViewFragment_to_categoryUsageFragment"
            app:destination="@id/categoryUsageFragment" />
        <action
            android:id="@+id/action_usageOverViewFragment_to_appUsageFragment"
            app:destination="@id/appUsageFragment" />
    </fragment>
    <fragment
        android:id="@+id/chooseMissionFragment"
        android:name="com.example.us0.choosemission.ChooseMission"
        android:label="fragment_choose_mission"
        tools:layout="@layout/fragment_choose_mission" >
        <action
            android:id="@+id/action_chooseMissionFragment_to_detailMission"
            app:destination="@id/detailMission" />
    </fragment>
    <fragment
        android:id="@+id/passageFragment"
        android:name="com.example.us0.home.PassageFragment"
        android:label="fragment_passage">
        <action
            android:id="@+id/action_passageFragment_to_chooseMissionFragment"
            app:destination="@id/chooseMissionFragment"
            app:popUpTo="@id/passageFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_passageFragment_to_askName"
            app:destination="@id/askName"
            app:popUpTo="@id/passageFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_passageFragment_to_lastMissionFragment"
            app:destination="@id/lastMissionFragment"
            app:popUpTo="@id/passageFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_passageFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/passageFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_passageFragment_to_rulesFragment2"
            app:destination="@id/rulesFragment2"
            app:popUpTo="@id/passageFragment"
            app:popUpToInclusive="true" />

    </fragment>
    <fragment
        android:id="@+id/askName"
        android:name="com.example.us0.home.askname.AskName"
        android:label="AskName" >
        <action
            android:id="@+id/action_askName_to_introToApp3"
            app:destination="@id/introToApp3" />
    </fragment>
    <fragment
        android:id="@+id/introToApp3"
        android:name="com.example.us0.home.appintro.IntroToApp"
        android:label="IntroToApp" >
        <action
            android:id="@+id/action_introToApp3_to_introToApp22"
            app:destination="@id/introToApp22" />
    </fragment>
    <fragment
        android:id="@+id/introToApp22"
        android:name="com.example.us0.home.appintro.IntroToApp2"
        android:label="IntroToApp2" >
        <action
            android:id="@+id/action_introToApp22_to_passageFragment"
            app:destination="@id/passageFragment" />
    </fragment>
    <fragment
        android:id="@+id/accomplishedMissionDetails"
        android:name="com.example.us0.home.closedmissions.AccomplishedMissionDetails"
        android:label="ClosedMissionsFragment" >
        <argument
            android:name="selectedMission"
            app:argType="com.example.us0.data.missions.DomainActiveMission" />
        <action
            android:id="@+id/action_accomplishedMissionDetails_to_sponsorDetailsFragment"
            app:destination="@id/sponsorDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/lastMissionFragment"
        android:name="com.example.us0.home.lastmission.LastMissionFragment"
        android:label="LastMissionFragment" >
        <action
            android:id="@+id/action_lastMissionFragment_to_passageFragment"
            app:destination="@id/passageFragment" />
        <action
            android:id="@+id/action_lastMissionFragment_to_reportFragment"
            app:destination="@id/reportFragment" />
        <action
            android:id="@+id/action_lastMissionFragment_to_lastMissionCompletedFragment"
            app:destination="@id/lastMissionCompletedFragment"
            app:popUpTo="@id/passageFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_lastMissionFragment_to_rulesFragment2"
            app:destination="@id/rulesFragment2" />
        <action
            android:id="@+id/action_lastMissionFragment_to_chooseMissionFragment"
            app:destination="@id/chooseMissionFragment" />
    </fragment>
    <fragment
        android:id="@+id/reportFragment"
        android:name="com.example.us0.home.report.ReportFragment"
        android:label="ReportFragment" >
        <action
            android:id="@+id/action_reportFragment_to_passageFragment"
            app:destination="@id/passageFragment" />
        <argument
            android:name="missionNumber"
            app:argType="integer"
            android:defaultValue="0" />
    </fragment>
    <fragment
        android:id="@+id/detailMission"
        android:name="com.example.us0.choosemission.DetailMission"
        android:label="DetailMission">
    <argument
        android:name="selectedMission"
        app:argType="com.example.us0.data.missions.DomainActiveMission" />

        <action
            android:id="@+id/action_detailMission_to_chooseMissionFragment"
            app:destination="@id/chooseMissionFragment" />
        <action
            android:id="@+id/action_detailMission_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_detailMission_to_rulesFragment2"
            app:destination="@id/rulesFragment2" />
        <argument
            android:name="showDifferentMissionButton"
            app:argType="boolean"
            android:defaultValue="true" />
        <action
            android:id="@+id/action_detailMission_to_sponsorDetailsFragment"
            app:destination="@id/sponsorDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/rulesFragment2"
        android:name="com.example.us0.home.rules.RulesFragment2"
        android:label="RulesFragment2" >
        <action
            android:id="@+id/action_rulesFragment2_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/navigation_to_sign_out"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.us0.home.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_permissionFragment"
            app:destination="@id/permissionFragment"
            app:popUpTo="@id/homeFragment"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_homeFragment_to_detailClosedMissionFragment"
            app:destination="@id/detailClosedMissionFragment" />
        <action
            android:id="@+id/action_homeFragment_to_rulesFragment2"
            app:destination="@id/rulesFragment2" />
        <action
            android:id="@+id/action_homeFragment_to_detailMission"
            app:destination="@id/detailMission" />
        <action
            android:id="@+id/action_homeFragment_to_chooseMissionFragment"
            app:destination="@id/chooseMissionFragment" />
        <action
            android:id="@+id/action_homeFragment_to_usageOverViewFragment"
            app:destination="@id/usageOverViewFragment" />
        <action
            android:id="@+id/action_homeFragment_to_featsFragment"
            app:destination="@id/featsFragment" />
        <action
            android:id="@+id/action_homeFragment_to_profileFragment"
            app:destination="@id/profileFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailClosedMissionFragment"
        android:name="com.example.us0.home.closedmissions.DetailClosedMissionFragment"
        android:label="fragment_deatil_closed_mission"
        tools:layout="@layout/fragment_deatil_closed_mission">
        <argument
            android:name="selectedMission"
            app:argType="com.example.us0.data.missions.DomainClosedMission"/>
        <action
            android:id="@+id/action_detailClosedMissionFragment_to_chooseMissionFragment"
            app:destination="@id/chooseMissionFragment" />
    </fragment>
    <fragment
        android:id="@+id/lastMissionCompletedFragment"
        android:name="com.example.us0.home.lastmission.LastMissionCompletedFragment"
        android:label="LastMissionCompletedFragment" >
        <action
            android:id="@+id/action_lastMissionCompletedFragment_to_passageFragment"
            app:destination="@id/passageFragment" />
        <argument
            android:name="missionNumber"
            app:argType="integer"
            android:defaultValue="0" />
    </fragment>
    <fragment
        android:id="@+id/FAQFragment"
        android:name="com.example.us0.home.navscreens.FAQFragment"
        android:label="FAQFragment" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.example.us0.home.navscreens.SettingsFragment"
        android:label="SettingsFragment" >
        <action
            android:id="@+id/action_settingsFragment_to_permissionFragment"
            app:destination="@id/permissionFragment" />
        <action
            android:id="@+id/action_settingsFragment_to_DOOAFragment"
            app:destination="@id/DOOAFragment" />
    </fragment>
    <fragment
        android:id="@+id/aboutFragment"
        android:name="com.example.us0.home.navscreens.AboutFragment"
        android:label="AboutFragment" />
    <fragment
        android:id="@+id/reportBugFragment"
        android:name="com.example.us0.home.navscreens.ReportBugFragment"
        android:label="ReportBugFragment" />
    <fragment
        android:id="@+id/featsFragment"
        android:name="com.example.us0.home.feats.FeatsFragment"
        android:label="FeatsFragment" >
        <action
            android:id="@+id/action_featsFragment_to_sponsorDetailsFragment"
            app:destination="@id/sponsorDetailsFragment" />
        <action
            android:id="@+id/action_featsFragment_to_detailMission"
            app:destination="@id/detailMission" />
        <action
            android:id="@+id/action_featsFragment_to_accomplishedMissionDetails"
            app:destination="@id/accomplishedMissionDetails" />
    </fragment>
    <fragment
        android:id="@+id/categoryUsageFragment"
        android:name="com.example.us0.home.usagestats.CategoryUsageFragment"
        android:label="CategoryUsageFragment" >
        <action
            android:id="@+id/action_categoryUsageFragment_to_appUsageFragment"
            app:destination="@id/appUsageFragment" />
        <argument
            android:name="catName"
            app:argType="string" />
        <action
            android:id="@+id/action_categoryUsageFragment_to_usageOverViewFragment"
            app:destination="@id/usageOverViewFragment" />
    </fragment>
    <fragment
        android:id="@+id/appUsageFragment"
        android:name="com.example.us0.home.usagestats.AppUsageFragment"
        android:label="AppUsageFragment" >
        <argument
            android:name="appPackageName"
            app:argType="string" />
        <action
            android:id="@+id/action_appUsageFragment_to_categoryUsageFragment"
            app:destination="@id/categoryUsageFragment" />
        <action
            android:id="@+id/action_appUsageFragment_to_usageOverViewFragment"
            app:destination="@id/usageOverViewFragment" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.example.us0.home.profile.ProfileFragment"
        android:label="ProfileFragment" >
        <action
            android:id="@+id/action_profileFragment_to_chooseMissionFragment"
            app:destination="@id/chooseMissionFragment" />
        <action
            android:id="@+id/action_profileFragment_to_yourPreviousMissionsFragment"
            app:destination="@id/yourPreviousMissionsFragment" />
    </fragment>
    <fragment
        android:id="@+id/yourPreviousMissionsFragment"
        android:name="com.example.us0.home.yourpreviousmissions.YourPreviousMissionsFragment"
        android:label="YourPreviousMissionsFragment" >
        <action
            android:id="@+id/action_yourPreviousMissionsFragment_to_profileFragment"
            app:destination="@id/profileFragment"
            app:popUpTo="@id/profileFragment"
            app:popUpToInclusive="true" />
        <argument
            android:name="currentMission"
            app:argType="com.example.us0.data.missions.DomainActiveMission" />
        <action
            android:id="@+id/action_yourPreviousMissionsFragment_to_detailMission"
            app:destination="@id/detailMission" />
        <action
            android:id="@+id/action_yourPreviousMissionsFragment_to_accomplishedMissionDetails"
            app:destination="@id/accomplishedMissionDetails" />
    </fragment>
    <fragment
        android:id="@+id/DOOAFragment"
        android:name="com.example.us0.drawoverotherapps.DOOAFragment"
        android:label="DOOAFragment" >
        <action
            android:id="@+id/action_DOOAFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            app:popUpTo="@id/navigation_to_sign_out"
            app:popUpToInclusive="true"/>
    </fragment>
    <fragment
        android:id="@+id/sponsorDetailsFragment"
        android:name="com.example.us0.choosemission.SponsorDetailsFragment"
        android:label="SponsorDetailsFragment" >

        <argument
            android:name="sponsorNumber"
            app:argType="integer"
            android:defaultValue="0" />
    </fragment>
</navigation>