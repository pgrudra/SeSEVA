<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable
            name="ContributionUpdateViewModel"
            type="com.spandverse.seseva.contributionupdate.ContributionUpdateViewModel" />
    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/common_background">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/go_to_home">
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".contributionupdate.ContributionUpdateActivity"
    android:paddingStart="16dp"
    android:paddingEnd="16dp">
<TextView
    android:id="@+id/t_1"
    android:layout_width="0dp"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    android:layout_marginTop="24dp"
    android:layout_height="wrap_content"
    app:layout_constraintTop_toTopOf="parent"
    style="@style/subtitle_1"
    android:text="@{ContributionUpdateViewModel.t1}"
    android:textColor="@color/colorSecondary"
    tools:text="Going good User"/>
    <TextView
        android:id="@+id/mission_name"
        android:layout_width="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/t_1"
        android:layout_marginTop="12dp"
        android:layout_height="wrap_content"
        style="@style/subtitle_2"
        android:text="@{ContributionUpdateViewModel.missionName}"
        android:textColor="@color/primary_text"
        tools:text="You are on Mission Name mission"/>
    <TextView
        android:id="@+id/sponsor_name"
        android:layout_width="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/mission_name"
        android:layout_marginTop="2dp"
        android:textColor="@color/disabled_text"
        style="@style/tertiary_text"
        android:text="@{ContributionUpdateViewModel.sponsorName}"
        android:layout_height="wrap_content"
        tools:text="aided by aid provider"/>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/image_view"
        android:layout_width="match_parent"
        android:layout_marginStart="40dp"
        android:layout_marginEnd="40dp"
        android:layout_height="140dp"
        app:layout_constraintTop_toBottomOf="@id/sponsor_name"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:scaleType="fitXY"
        tools:srcCompat="@tools:sample/backgrounds/scenic"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/contribution_c_l"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/all_corner_rounded_4dp_1e"
        app:layout_constraintTop_toBottomOf="@id/image_view"
        android:layout_marginTop="16dp"
        android:paddingBottom="16dp">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/yda_reward_c_l"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            android:onClick="@{()->ContributionUpdateViewModel.rewardExpandOrContract()}"
            android:padding="4dp"
            android:layout_marginTop="12dp">
            <TextView
                android:id="@+id/yda_reward_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="12dp"
                app:layout_constraintTop_toTopOf="parent"
                android:text="@string/yesterday_s_reward"
                android:textColor="@color/primary_text"
                style="@style/subtitle_1"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/reward_e_c_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="4dp"
                app:layout_constraintTop_toTopOf="@id/yda_reward_text"
                app:layout_constraintBottom_toBottomOf="@id/yda_reward_text"
                app:layout_constraintEnd_toEndOf="parent"
                android:src="@drawable/ic_expand_vector"
                android:background="@null"/>

            <TextView
                android:id="@+id/yda_reward_i2"
                style="@style/subtitle_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@{ContributionUpdateViewModel.ydaReward}"
                android:textColor="@color/rules_card"
                app:layout_constraintBottom_toBottomOf="@id/yda_reward_text"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.36"
                app:layout_constraintStart_toEndOf="@id/yda_reward_text"
                app:layout_constraintTop_toTopOf="@id/yda_reward_text"
                tools:text="Rs 25" />
            <TextView
                android:id="@+id/yda_reward"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:text="Rs 25"
                android:textColor="@color/primary_text"
                style="@style/subtitle_1"
                android:text="@{ContributionUpdateViewModel.ydaReward}"
                app:layout_constraintTop_toTopOf="@id/yda_reward_text"
                app:layout_constraintBottom_toBottomOf="@id/yda_reward_text"
                app:layout_constraintStart_toStartOf="@id/yda_reward_i2"/>
            <TextView
                android:id="@+id/daily_reward_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone"
                app:layout_constraintTop_toBottomOf="@id/yda_reward_text"
                app:layout_constraintEnd_toEndOf="@id/yda_reward_text"
                android:text="@string/daily_reward_text"
                android:textColor="@color/disabled_text"
                style="@style/tertiary_text"/>
            <TextView
                android:id="@+id/weekly_reward_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone"
                app:layout_constraintTop_toBottomOf="@id/daily_reward_text"
                app:layout_constraintEnd_toEndOf="@id/yda_reward_text"
                android:text="@string/weekly_reward_text"
                android:textColor="@color/disabled_text"
                style="@style/tertiary_text"/>
            <TextView
                android:id="@+id/daily_reward"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone"
                app:layout_constraintTop_toBottomOf="@id/yda_reward_text"
                app:layout_constraintStart_toStartOf="@id/yda_reward"
                tools:text="Rs 25"
                android:text="@{ContributionUpdateViewModel.dailyReward}"
                android:textColor="@color/disabled_text"
                style="@style/tertiary_text"/>
            <TextView
                android:id="@+id/weekly_reward"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone"
                android:text="@{ContributionUpdateViewModel.weeklyReward}"
                app:layout_constraintTop_toBottomOf="@id/daily_reward_text"
                app:layout_constraintStart_toStartOf="@id/yda_reward"
                tools:text="Rs 10"
                android:textColor="@color/disabled_text"
                style="@style/tertiary_text"/>


        </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/penalty_c_l"
                android:paddingStart="4dp"
                android:paddingTop="4dp"
                android:paddingEnd="4dp"
                android:onClick="@{()->ContributionUpdateViewModel.penaltyExpandOrContract()}"
                app:layout_constraintTop_toBottomOf="@+id/yda_reward_c_l">
                <TextView
                    android:id="@+id/yda_reward_text_i"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="12dp"
                    app:layout_constraintTop_toTopOf="parent"
                    android:text="@string/yesterday_s_reward"
                    android:textColor="@color/rules_card"
                    style="@style/subtitle_1"/>
                <TextView
                    android:id="@+id/tot_penalty_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    style="@style/subtitle_1"
                    android:textColor="@color/colorError"
                    android:text="@string/total_penalty_text"
                    app:layout_constraintEnd_toEndOf="@id/yda_reward_text_i"/>

                <TextView
                    android:id="@+id/yda_reward_i"
                    style="@style/subtitle_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@{ContributionUpdateViewModel.ydaReward}"
                    android:textColor="@color/rules_card"
                    app:layout_constraintBottom_toBottomOf="@id/tot_penalty_text"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.36"
                    app:layout_constraintStart_toEndOf="@id/tot_penalty_text"
                    app:layout_constraintTop_toTopOf="@id/tot_penalty_text"
                    tools:text="Rs 25" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    tools:text="Rs 13"
                    android:textColor="@color/colorError"
                    style="@style/subtitle_1"
                    android:text="@{ContributionUpdateViewModel.totPenalty}"
                    app:layout_constraintTop_toTopOf="@id/tot_penalty_text"
                    app:layout_constraintBottom_toBottomOf="@id/tot_penalty_text"
                    app:layout_constraintStart_toStartOf="@id/yda_reward_i"/>
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/penalty_e_c_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="4dp"
                    app:layout_constraintTop_toTopOf="@id/tot_penalty_text"
                    app:layout_constraintBottom_toBottomOf="@id/tot_penalty_text"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:src="@drawable/ic_expand_vector"
                    android:background="@null"/>
            </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/penalty_items_c_l"
            android:visibility="gone"
                app:layout_constraintTop_toBottomOf="@+id/penalty_c_l">

            <TextView
                android:id="@+id/yda_reward_i3"
                style="@style/subtitle_1"
                android:layout_width="wrap_content"
                android:layout_height="10dp"
                android:layout_marginEnd="4dp"
                android:text="@{ContributionUpdateViewModel.ydaReward}"
                android:textColor="@color/rules_card"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.36"
                app:layout_constraintStart_toEndOf="@id/yda_reward_text_i3"
                app:layout_constraintTop_toTopOf="parent"
                tools:text="Rs 25" />
            <TextView
                    android:id="@+id/yda_reward_text_i3"
                    android:layout_width="wrap_content"
                    android:layout_height="10dp"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="16dp"
                    app:layout_constraintTop_toTopOf="parent"
                    android:text="@string/yesterday_s_reward"
                    android:textColor="@color/rules_card"
                    style="@style/subtitle_1"/>
             <TextView
                    android:id="@+id/social_penalty_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="@id/yda_reward_text_i3"
                    android:text="@string/social_text"
                 android:visibility="gone"
                    android:textColor="@color/disabled_text"
                    style="@style/tertiary_text"/>
                <TextView
                    android:id="@+id/video_penalty_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/social_penalty_text"
                    app:layout_constraintEnd_toEndOf="@id/yda_reward_text_i3"
                    android:text="@string/video_comics"
                    android:visibility="gone"
                    android:textColor="@color/disabled_text"
                    style="@style/tertiary_text"/>
                <TextView
                    android:id="@+id/games_penalty_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/video_penalty_text"
                    app:layout_constraintEnd_toEndOf="@id/yda_reward_text_i3"
                    android:text="@string/games_text"
                    android:visibility="gone"
                    android:textColor="@color/disabled_text"
                    style="@style/tertiary_text"/>
                <TextView
                    android:id="@+id/com_penalty_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/games_penalty_text"
                    app:layout_constraintEnd_toEndOf="@id/yda_reward_text_i3"
                    android:text="@string/comm_browsing"
                    android:visibility="gone"
                    android:textColor="@color/disabled_text"
                    style="@style/tertiary_text"/>
                <TextView
                    android:id="@+id/msnbs_penalty_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    app:layout_constraintTop_toBottomOf="@id/com_penalty_text"
                    app:layout_constraintEnd_toEndOf="@id/yda_reward_text_i3"
                    android:text="@string/msnbs_text"
                    android:textColor="@color/disabled_text"
                    style="@style/tertiary_text"/>
            <TextView
                android:id="@+id/entertainment_penalty_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone"
                app:layout_constraintTop_toBottomOf="@id/msnbs_penalty_text"
                app:layout_constraintEnd_toEndOf="@id/yda_reward_text_i3"
                android:text="@string/entertainment"
                android:textColor="@color/disabled_text"
                style="@style/tertiary_text"/>
                <TextView
                    android:id="@+id/others_penalty_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    app:layout_constraintTop_toBottomOf="@id/others_penalty_text"
                    app:layout_constraintEnd_toEndOf="@id/yda_reward_text_i3"
                    android:text="@string/other_apps_text"
                    android:textColor="@color/disabled_text"
                    style="@style/tertiary_text"/>

                <TextView
                    android:id="@+id/social_penalty"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="@id/yda_reward_i3"
                    tools:text="Rs 8"
                    android:text="@{ContributionUpdateViewModel.socialPenalty}"
                    android:textColor="@color/disabled_text"
                    style="@style/tertiary_text"/>
                <TextView
                    android:id="@+id/video_penalty"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    app:layout_constraintTop_toBottomOf="@id/social_penalty"
                    app:layout_constraintStart_toStartOf="@id/yda_reward_i3"
                    tools:text="Rs 8"
                    android:text="@{ContributionUpdateViewModel.videoPenalty}"
                    android:textColor="@color/disabled_text"
                    style="@style/tertiary_text"/>
                <TextView
                    android:id="@+id/games_penalty"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    app:layout_constraintTop_toBottomOf="@id/video_penalty"
                    app:layout_constraintStart_toStartOf="@id/yda_reward_i3"
                    tools:text="Rs 8"
                    android:text="@{ContributionUpdateViewModel.gamesPenalty}"
                    android:textColor="@color/disabled_text"
                    style="@style/tertiary_text"/>
                <TextView
                    android:id="@+id/com_penalty"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    app:layout_constraintTop_toBottomOf="@id/games_penalty"
                    app:layout_constraintStart_toStartOf="@id/yda_reward_i3"
                    tools:text="Rs 8"
                    android:text="@{ContributionUpdateViewModel.comPenalty}"
                    android:textColor="@color/disabled_text"
                    style="@style/tertiary_text"/>
                <TextView
                    android:id="@+id/msnbs_penalty"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    app:layout_constraintTop_toBottomOf="@id/com_penalty"
                    app:layout_constraintStart_toStartOf="@id/yda_reward_i3"
                    tools:text="Rs 8"
                    android:text="@{ContributionUpdateViewModel.msnbsPenalty}"
                    android:textColor="@color/disabled_text"
                    style="@style/tertiary_text"/>
            <TextView
                android:id="@+id/entertainment_penalty"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone"
                app:layout_constraintTop_toBottomOf="@id/msnbs_penalty"
                app:layout_constraintStart_toStartOf="@id/yda_reward_i3"
                tools:text="Rs 8"
                android:text="@{ContributionUpdateViewModel.entertainmentPenalty}"
                android:textColor="@color/disabled_text"
                style="@style/tertiary_text"/>
            <TextView
                    android:id="@+id/others_penalty"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    app:layout_constraintTop_toBottomOf="@id/others_penalty"
                    app:layout_constraintStart_toStartOf="@id/yda_reward_i3"
                    tools:text="Rs 8"
                    android:textColor="@color/disabled_text"
                    android:text="@{ContributionUpdateViewModel.othersPenalty}"
                    style="@style/tertiary_text"/>

        </androidx.constraintlayout.widget.ConstraintLayout>
        <View
            android:id="@+id/v1"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"
            app:layout_constraintTop_toBottomOf="@id/penalty_items_c_l"
            android:layout_marginTop="12dp"
            android:background="@color/selected_group_card_background"/>

            <TextView
                android:id="@+id/you_raised_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                app:layout_constraintTop_toBottomOf="@id/v1"
                android:textColor="@color/colorPrimary"
                android:textSize="24sp"
                android:fontFamily="@font/montserrat_used"
                android:text="@string/you_raised"
                app:layout_constraintEnd_toEndOf="@id/yda_reward_text_i2"/>
        <TextView
            android:id="@+id/yda_reward_text_i2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"
            app:layout_constraintTop_toTopOf="parent"
            android:text="@string/yesterday_s_reward"
            android:textColor="@color/rules_card"
            style="@style/subtitle_1"/>

        <TextView
            android:id="@+id/reward_i2"
            style="@style/subtitle_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="4dp"
            android:text="@{ContributionUpdateViewModel.ydaReward}"
            android:textColor="@color/rules_card"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.36"
            app:layout_constraintStart_toEndOf="@id/yda_reward_text_i2"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="Rs 25" />

        <TextView
            android:id="@+id/you_raised"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:text="Rs 12"
            android:textColor="@color/colorPrimary"
            android:textSize="24sp"
            android:text="@{ContributionUpdateViewModel.youRaisedAmount}"
            app:layout_constraintStart_toStartOf="@id/reward_i2"
            android:fontFamily="@font/montserrat_used"
            app:layout_constraintTop_toTopOf="@id/you_raised_text"
           app:layout_constraintBottom_toBottomOf="@id/you_raised_text"/>
<TextView
    android:id="@+id/raised_nothing_text"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center"
    android:visibility="gone"
    app:layout_constraintTop_toBottomOf="@id/v1"
    android:text="@{ContributionUpdateViewModel.raisedNothingText"
    android:layout_marginStart="8dp"
    android:layout_marginEnd="8dp"
    android:layout_marginTop="9dp"
    style="@style/caption"
    android:textColor="@color/colorSecondary"
    tools:text="You raised nothing as total penalty exceeded yesterday's reward"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/usage_c_l"
        android:layout_marginTop="24dp"
        android:paddingStart="8dp"
        android:paddingEnd="8dp"
        android:onClick="@{()->ContributionUpdateViewModel.usageExpandOrContract()}"
        app:layout_constraintTop_toBottomOf="@id/contribution_c_l">
        <TextView
            android:id="@+id/usage_heading"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/yesterday_s_usage_stats"
            android:textColor="@color/primary_text"
            style="@style/subtitle_1"
            app:layout_constraintEnd_toStartOf="@id/usage_e_c_icon"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/usage_e_c_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/ic_expand_vector"
            app:layout_constraintTop_toTopOf="@id/usage_heading"
            app:layout_constraintBottom_toBottomOf="@id/usage_heading"
            app:layout_constraintEnd_toEndOf="parent"/>
        <TextView
            android:id="@+id/tot_time"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            tools:text="Total Screen time: 2hrs 30 mins"
            android:textColor="@color/disabled_text"
            style="@style/caption"
            android:layout_marginTop="4dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:text="@{ContributionUpdateViewModel.totTime}"
            app:layout_constraintTop_toBottomOf="@id/usage_heading"/>
        <TextView
            android:id="@+id/tot_lanches"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/tot_time"
            android:layout_marginTop="4dp"
            android:text="@{ContributionUpdateViewModel.totLaunches}"
            android:textColor="@color/disabled_text"
            app:layout_constraintStart_toStartOf="@id/tot_time"
            app:layout_constraintEnd_toEndOf="parent"
            tools:text="Total App launches: 123"
            style="@style/caption"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/usage_details_c_l"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        android:paddingBottom="28dp"
        app:layout_constraintTop_toBottomOf="@id/usage_c_l">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/category_text"
            style="@style/tertiary_text"
            android:id="@+id/category_title"
            android:layout_marginStart="8dp"
            android:textColor="@color/primary_text"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp"/>

        <TextView
            android:id="@+id/app_launches_title"
            style="@style/tertiary_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="8dp"
            android:text="@string/app_launches_text"
            android:textColor="@color/primary_text"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/screen_time_title"
